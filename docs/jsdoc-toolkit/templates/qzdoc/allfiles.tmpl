<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
	<head>
		<meta http-equiv="content-type" content="text/html; charset={+IO.encoding+}" />
		{! Link.base = ""; /* all generated links will be relative to this */ !}
		<title>JsDoc Reference - File Index</title>
		<meta name="generator" content="JsDoc Toolkit" />
		
		<link rel="stylesheet" href="http://qzone.isd.com/css/style.css" type="text/css"/>
		<link rel="stylesheet" href="http://qzone.isd.com/css/items.css" type="text/css"/>
	</head>
	
	<body>
	   <div id="main">
		    {+include("static/header.html")+}
		
    		<div id="content">
				<!-- 内容区域 -->
				<div class="items_row">
					<div class="item">
						<h1 class="title articleTitle">QZFL File Index</h1>
						<div class="detail articleDetail">
							<!-- 正文部分 -->                            
							<for each="item" in="data">
                            <div>
                                <h3>
                                 {!
                                   var fileLink = new Link().toSrc(item.alias).withText(item.name);
                                   fileLink = fileLink.toString().replace(/\>/,'target="_blank" class="blank">');
                                 !}
                                 {+fileLink+}
                                </h3>
                                <if test="item.desc">{+resolveLinks(item.desc)+}</if>
                                <dl>
                                    <if test="item.author">
                                        <dt class="heading">Author:</dt>
                                        <dd>{+item.author+}</dd>
                                    </if>
                                    <if test="item.version">
                                        <dt class="heading">Version:</dt>
                                        {!
                                            var version = item.version.toString().slice(8,-2);
                                        !}
                                            <dd>{+version+}</dd>
                                    </if>
                                    {! var locations = item.comment.getTag('location').map(function($){return $.toString().replace(/(^\$ ?| ?\$$)/g, '').replace(/^HeadURL: https:/g, 'http:');}) !}
                                    <if test="locations.length">
                                        <dt class="heading">Location:</dt>
                                            <for each="location" in="locations">
                                            <dd><a href="{+location+}">{+location+}</a></dd>
                                            </for>
                                    </if>
                                </dl>
                            </div>
                            <hr />
                            </for>

 							<!-- 正文部分结束 -->
						</div>
					</div>
				</div>
			</div>
			
        </div>
        
        <div id="footer">
            <div class="foot">
                <div><a href="#" target="_blank">关于</a> | <a href="http://km.oa.com" target="_blank">KM平台</a> | <a href="http://20050606.qzone.qq.com" target="_blank">官方空间</a> | <a href="http://qzone.qq.com" target="_blank">空间首页</a> </div>
                <div class="copyright">copyright 2009，by Qzone Platform Develop Team </div>
                <div class="fineprint" style="margin-top:10px">
                    Documentation generated by <a href="http://code.google.com/p/jsdoc-toolkit/" target="_blankt">JsDoc Toolkit</a> {+JSDOC.VERSION+}<if test="JSDOC.opt.D.copyright"> &copy;{+JSDOC.opt.D.copyright+}<br /></if> on {+new Date()+}
                </div>
            </div>
        </div>
	</body>
</html>